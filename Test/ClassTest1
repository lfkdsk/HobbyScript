class lfkdsk {


    Ss = ll = 100;
    Ssdut = closure(){
        Ss = 1000;
    };

    function FuckIt(i){
        Ss = i;
        Ss;
    };
}

lfk = "";
// 提供了两种写法 构造带括号 就会使用构造
// 不带括号就要没有构造
lfi = lfkdsk.Initial;

lfi.Ssdut();
// lfi.ll;

logInfo(lfi.Ss + lfk);

lfi.FuckIt(10000);

logInfo(lfi.Ss + lfk);